[sub_resource type="Shader" id=6]
code = "shader_type canvas_item;

uniform vec4 replace_color: hint_color;
uniform vec4 modified_color: hint_color;
uniform vec4 other_modified_color: hint_color;
uniform float charge_amount: hint_range(0., 1.);

void fragment(){
	vec4 color = texture(TEXTURE, UV);
	if (color == modified_color || color == other_modified_color) {
		COLOR = color;
		if ((1. - UV.y) < charge_amount){
			COLOR.a = 1.0;
		} else {
			COLOR = replace_color;
		}
	} else {
		COLOR.a = 1.0;
		COLOR = color;
	}
}"

